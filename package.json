{
  "name": "php-react-ui",
  "version": "0.1.0",
  "description": "A framework agnostic React UI boilerplate for PHP projects",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "webpack.config.js",
  "scripts": {
    "start": "gulp --gulpfile ui/gulpfile.js start",
    "dev": "gulp --gulpfile ui/gulpfile.js dev",
    "clean": "$(npm bin)/rimraf var/www/dist && mkdir var/www/dist",
    "build": "npm run clean && BABEL_ENV=production $(npm bin)/webpack -p --progress --config ui/webpack.config.prod.js",
    "test": "$(npm bin)/karma start ui/karma.conf.js",
    "lint": "$(npm bin)/eslint ui/src; exit 0;",
    "php": "gulp --gulpfile ui/gulpfile.js watch-php"
  },
  "peerDependencies": {
    "react": ">=15.0.2",
    "react-dom": ">=15.0.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-airbnb": "^1.1.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.9.2",
    "browser-sync": "^2.11.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-exists": "^1.0.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-connect-php": "0.0.7",
    "gulp-phpcs": "^1.1.1",
    "gulp-phpmd": "^2.0.1",
    "gulp-phpmd-plugin": "^1.1.3",
    "gulp-phpunit": "^0.14.0",
    "imports-loader": "^0.6.5",
    "jscs": "^3.0.3",
    "jsdom": "^8.4.0",
    "jsx-loader": "^0.13.2",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "react-proxy": "^1.1.8",
    "react-transform-catch-errors": "^1.0.2",
