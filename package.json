{
  "name": "php-reactjs-ui",
  "version": "0.2.0",
  "description": "A framework agnostic React UI boilerplate for PHP projects",
  "scripts": {
    "start": "gulp --gulpfile ui/gulpfile.js start",
    "dev": "gulp --gulpfile ui/gulpfile.js dev",
    "build": "BABEL_ENV=production $(npm bin)/webpack -p --progress --config ui/webpack.config.prod.js",
    "test": "$(npm bin)/karma start ui/karma.conf.js",
    "lint": "$(npm bin)/eslint ui/src --ignore-pattern *-compiled.js; exit 0;",
    "php": "gulp --gulpfile ui/gulpfile.js watch-php"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koriym/php-reactjs-ui.git"
  },
  "keywords": [
    "reactjs",
    "boilerplate",
    "php",
    "hmr",
    "webpack"
  ],
  "author": "Akihito Koriyama <akihito.koriyama> (http://github.com/koriym)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koriym/php-reactjs-ui/issues"
  },
  "homepage": "https://github.com/koriym/php-reactjs-ui",
  "dependencies": {
    "react": ">=15.0.2",
    "react-dom": ">=15.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "browser-sync": "^2.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs": "^0.8.4",
    "file-exists": "^2.0.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-connect-php": "0.0.8",
    "gulp-phpcs": "^1.1.1",
    "gulp-phpmd": "^2.0.1",
    "gulp-phpmd-plugin": "^1.1.4",
    "gulp-phpunit": "^0.18.0",
    "imports-loader": "^0.6.5",
    "jscs": "^3.0.7",
    "jsdom": "^9.5.0",
    "jsx-loader": "^0.13.2",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.2",
    "object-assign": "^4.1.0",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "sizzle": "^2.3.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
